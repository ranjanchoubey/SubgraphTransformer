{
  "gpu": {
    "use": true,
    "id": 0
},

"model": "GraphTransformer",
"dataset": "Cora",
"out_dir": "out/",

"params": {
  "seed": 41,
  "epochs": 100,
  "batch_size": 32,
  "init_lr": 0.0005,
  "lr_reduce_factor": 0.5,
  "lr_schedule_patience": 15,
  "min_lr": 1e-6,
  "weight_decay": 0.005,
  "print_epoch_interval": 5,
  "max_time": 24
},

"net_params": {
  "L": 6,
  "n_heads": 8,
  "hidden_dim": 128,  
  "out_dim": 128,     
  "residual": true,
  "readout": "mean",
  "in_feat_dropout": 0.0,
  "dropout": 0.2,
  "layer_norm": true,
  "batch_norm": true,
  "self_loop": false,
  "lap_pos_enc": false,
  "pos_enc_dim": 8,
  "wl_pos_enc": true,
  "full_graph": false
},

"data":
{
  "num_parts":100
},

"gcn":
{
  "input_dim":1433,
  "hidden_dim":64,
  "output_dim":16   
}



}