{
    "gpu": {
        "use": true,
        "id": 0
    },
    "model": "GraphTransformer",
    "dataset": "CoraFull",
    "out_dir": "out/",
    "params": {
        "seed": 41,
        "epochs": 800,
        "batch_size": 1,
        "init_lr": 0.0007,
        "lr_reduce_factor": 0.5,
        "lr_schedule_patience": 10,
        "min_lr": 1e-7,
        "weight_decay": 0.0,
        "max_time": 12
    },
    "net_params": {
        "L": 4,
        "n_heads": 8,
        "hidden_dim": 512,    
        "out_dim": 512,
        "edge_feat": false,
        "residual": true,
        "readout": "mean",
        "in_feat_dropout": 0.2,
        "dropout": 0.2,
        "layer_norm": true,
        "batch_norm": true,
        "self_loop": false,
        "lap_pos_enc": false,
        "pos_enc_dim": 8,
        "wl_pos_enc": false,
        "full_graph": true
    },
    "data": {
        "num_parts": 500,
        "num_classes": 70
    },
    "gcn": {
        "input_dim": 8710,
        "hidden_dim": 1024,
        "output_dim": 32
}
}
