{
  "gpu": {
    "use": true,
    "id": 0
},

"model": "GraphTransformer",
"dataset": "CoraSmall",
"out_dir": "out/",

"params": {
  "seed": 41,
  "epochs": 500,
  "batch_size": 128,
  "init_lr": 0.0007,
  "lr_reduce_factor": 0.5,
  "lr_schedule_patience": 15,
  "min_lr": 1e-6,
  "weight_decay": 0.0,
  "print_epoch_interval": 5,
  "max_time": 24
},

"net_params": {
  "L": 10,
  "n_heads": 16,
  "hidden_dim": 64,
  "out_dim": 64,
  "edge_feat": false,
  "residual": true,
  "readout": "mean",
  "in_feat_dropout": 0.0,
  "dropout": 0.01,
  "layer_norm": false,
  "batch_norm": true,
  "self_loop": true,
  "lap_pos_enc": false,
  "pos_enc_dim": 8,
  "wl_pos_enc": false,
  "full_graph": false,

  "num_subgraph" :80,
  "reg_lambda": 0.001

},

"data":
{
  "num_classes": 7
},

"gcn":
{
  "input_dim":1433,
  "hidden_dim":256,
  "output_dim":32
}

}